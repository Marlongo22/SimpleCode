print('Welcome to the automatize compound calculator')
mg=input('The value of Mg is ')
s=input('The value of S is ')
o=input('The value of O is ')
mg_s=round(float(mg)/24/(float(mg)/24))
s_s=round(float(s)/32/(float(mg)/24))
o_s=round(float(o)/16/(float(mg)/24))
print(f'The compound is Mg{str(mg_s)}S{str(s_s)}O{str(o_s)}')

