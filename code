def car_game():
    command = ""
    print("welcome to the car game, please enter 'help' ")
    while True:
        command = input('> ').lower()
        if command == 'start':
            print('car started')
            command = input('> ')
            if command == 'start':
                print('car already started')
        elif command == 'stop':
            print('car stopped')
            command = input('> ')
            if command == 'stop':
                print('car already stopped')
        elif command == 'help':
            print('''
    start - to start the car
    stop - to stop the car
    quit - to quit the game
    ''')
        elif command == 'quit':
            break
        else:
            print("speak well bastard")



class Hello:
    def __init__(self, number, x, y):
        self.super = number * 2
        self.x = x
        self.y = y

    def hello_function(self):
        print(f'hello im a function {self.super}')


hello = Hello(5, 7, 3)
hello.hello_function()
x = hello.x
y = hello.y
print(f'({x}, {y})')


from modules import find_max
numbers = [3, 7, 8, 4, 5]
print(find_max(numbers))


