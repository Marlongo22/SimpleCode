help_ = 'help'
stop_ = 'stop'
start_ = 'start'
quit_ = 'quit'
tries = 0
while tries < 5:
    starting_command = input('> beg for help ')
    print('''
    Start - To start the car
    Stop  - To stop the car
    Quit  - To exit
    ''')
    while tries < 5:
        game_command = input('> what do you want ')
        if game_command.lower() == 'start':
            print('car started')
        elif game_command.lower() == 'stop':
            print('car stopped')
        elif game_command.lower() == 'quit':
            break
        else:
            print("i don't understand you")
else:
    print("you quit")




