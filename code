tries = 0
print('Bienvenido al juego del carrito')
while tries < 5:
    starting_command = input('> RUEGA POR MI AYUDA! ')
    while starting_command.lower() == 'ayuda':
        print('''
        Iniciar - Para iniciar el auto
        Parar   - Para parar el auto
        Salir   - Para Salir del juego
        ''')
        game_command = input('> Elige una opcion ')
        if game_command.lower() == 'iniciar':
            print(input('El carro ha iniciado'))
        elif game_command.lower() == 'parar':
            print(input('El carro ha parado'))
        elif game_command.lower() == 'salir':
            print('Saliste! :(')
            tries = 6
            break
        else:
            print(input('escriba bien'))


command = ""
while True:
    command = input('> ').lower()
    if command == 'start':
        print('car started')
        command = input('> ')
        if command == 'start':
            print('car already started')
    elif command == 'stop':
        print('car stopped')
        command = input('> ')
        if command == 'stop':
            print('car already stopped')
    elif command == 'help':
        print('''
start - to start the car
stop - to stop the car
quit - to quit the game
''')
    elif command == 'quit':
        break
    else:
        print("i don't understand you")

